{
  "name": "ref-tool-worker",
  "compatibility_date": "2025-11-12",
  "main": "dist/worker.js",
  "build": {
    "command": "npm run build",
    "watch_dir": "src"
  },
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "observability": {
  "logs": {
    "enabled": true,
    "head_sampling_rate": 1,
    "invocation_logs": true,
    "persist": true
    }
  }
}